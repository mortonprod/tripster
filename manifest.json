{
  "manifest_version":2,
  "version":"1.0",
  "name":"Tripster-Test",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background" : {
    "scripts" : ["background.js"]
  },
  "content_scripts":[
   {
    "matches":["<all_urls>"],
    "js":["content.js"]
   }
  ],
  "web_accessible_resources": [
        "iframe.html"
  ]
}