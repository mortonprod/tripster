{
  "name": "Tripster",
  "description": "Example manipulation of all images on a webpage using HTML canvas and web assembly. The end result is a kaleidoscope of fun!",
  "version": "0.0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": ["storage"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "sandbox": {
    "pages": [
      "iframe.html"
    ]
  },
  "content_security_policy": {
    "sandbox": "sandbox allow-scripts script-src 'self' 'unsafe-eval'; img-src data:"
  },
  "web_accessible_resources": [
    {
      "resources": ["iframe.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_title": "Tripster",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/16x.png",
      "32": "/icons/32x.png",
      "48": "/icons/48x.png",
      "128": "/icons/128x.png"
    }
  },
  "icons": {
    "16": "/icons/16x.png",
    "32": "/icons/32x.png",
    "48": "/icons/48x.png",
    "128": "/icons/128x.png"
  }
}